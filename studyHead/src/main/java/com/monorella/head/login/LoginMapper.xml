<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.monorella.head.login.LoginMapper">
	<select id="LoginPro" parameterType="java.util.Map" resultType="com.monorella.head.dto.Login">
		SELECT head_id, head_pw
		FROM head_manager_tb
		WHERE head_id=#{head_id}
		AND head_pw=#{head_pw}
	</select>
</mapper>